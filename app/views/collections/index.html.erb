<h1>Collections#index</h1>
<p>Find me in app/views/collections/index.html.erb</p>

<%= link_to('new', new_collection_path) %>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>bibliography ID</th>
      <th>作成日</th>
      <th>最終更新日</th>
      <th>詳細表示</th>
      <th>編集</th>
    </tr>
  </thead>
  <tbody><% @collections.each do |c| %>
    <tr>
      <td><%= c.id %></td>
      <td><%= c.bibliography_id %></td>
      <td><%= c.created_at %></td>
      <td><%= c.updated_at %></td>
      <td><%= link_to('show', collection_path(c)) %></td>
      <td><%= link_to('edit', edit_collection_path(c)) %></td>
    </tr>
  <% end %></tbody>
</table>

